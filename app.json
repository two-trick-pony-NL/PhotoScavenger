{
  "expo": {
    "name": "photoscavenger",
    "slug": "PhotoScavenger",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#d90827"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544~1458002511"}, // sample id, replace with your own
      "supportsTablet": false,
      "buildNumber": "3",
      "bundleIdentifier": "com.petervandoorn.PhotoScavenger",
      "infoPlist": {
        "NSCameraUsageDescription": "Photo Scavenger uses the camera to photograph objects in the game.",
        "NSPhotoLibraryAddUsageDescription": "Allow Photo Scavenger to save the photos while playing the game.",
        "NSPhotoLibraryUsageDescription": "Allow Photo Scavenger to save the photos while playing the game."
      }
    },
    "android": {
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544~1458002511"}, // sample id, replace with your own
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF",
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}