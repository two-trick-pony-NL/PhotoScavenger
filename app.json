{
  "expo": {
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Photo Scavenger uses the Camera, to photograph objects while playing the game.",
          "savePhotosPermission": "Allow Photo Scavenger to save photos you take during the game."
        }
      ]
    ],
    "name": "photoscavenger",
    "slug": "PhotoScavenger",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#d90827"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.petervandoorn.PhotoScavenger",
    },
    "android": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF",
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
